{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "es5",
      "es2015.promise"
    ],
    "module": "commonjs",
    "declaration": true,
    "sourceMap": true,
    "outDir": "./dist/",
    "rootDir": "./src/",
    "removeComments": true,
    "downlevelIteration": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
  }
}